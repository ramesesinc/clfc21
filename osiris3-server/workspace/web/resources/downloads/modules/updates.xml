<app>
    <env>
		app.title=CLFC 2.0
        app.host=localhost:8070
		app.cluster=osiris3
        app.context=clfc
		app.help=http://localhost:8080/help
		app.loader=com.rameses.osiris2.client.OsirisAppLoader
		readTimeout=50000
		connectionTimeout=5000
    </env>    
    <modules>
		<!--  osiris3 core libraries -->
		<module file="core/osiris2-client-themes.jar" version="1.9"/>
		<module file="core/osiris2-client-ui.jar" version="1.9"/>
		<module file="core/osiris2-framework.jar" version="1.9"/>
		<module file="core/osiris2-report.jar" version="2.0"/>
		<module file="core/rameses-client-ui.jar" version="2.2"/>
		<module file="core/rameses-common2.jar" version="2.2"/>
		<module file="core/rameses-custom-impl.jar" version="2.0"/>

   		<module file="core/rameses-client-ui-bio.jar" version="1.1"/>
   		<module file="core/rameses-jdbc16.jar" version="1.0"/>
   		<module file="core/rameses-seti.jar" version="1.0"/>

		<!-- client shared modules -->
		<module file="shared/client-system.jar" version="1.5"/>
		<module file="shared/client-ui-extended.jar" version="2.1"/>
		<module file="shared/rameses-client-menu.jar" version="1.8"/>
		<module file="shared/rameses-client-report.jar" version="1.5"/>
		<module file="shared/rameses-enterprise-admin.jar" version="1.5"/>
		<module file="shared/rameses-enterprise-entity.jar" version="1.5"/>
		<module file="shared/rameses-rules-mgmt.jar" version="1.3"/>
		<module file="shared/rameses-workflow-mgmt.jar" version="1.3"/>

