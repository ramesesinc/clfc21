   		<module file="shared/rameses-enterprise.jar" version="1.0"/>


		<!-- main-app -->
		<module file="rameses-clfc-main.jar" version="1.6"/>
		
		<!-- plugins -->
		<module file="rameses-clfc-customer.jar" version="2.3"/>
		<module file="rameses-clfc-datamgmt.jar" version="3.4"/>
		<module file="rameses-clfc-loan.jar" version="8.2"/>
		<module file="rameses-clfc-report.jar" version="6.2"/>
		<module file="rameses-clfc-patch.jar" version="4.8"/>
		<module file="rameses-clfc-treasury.jar" version="6.1"/>
